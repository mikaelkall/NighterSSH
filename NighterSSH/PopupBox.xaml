<Window x:Class="NighterSSH.PopupBox"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="PopupBox" Height="229.5" Width="448.833" WindowStartupLocation="CenterScreen" Opacity="0.9" AllowsTransparency="True" WindowStyle="None" Background="{x:Null}" Visibility="Visible" MouseDown="Window_MouseDown"
        DataContext="{Binding RelativeSource={RelativeSource Self}}">
    

	<Grid>
		<Rectangle Fill="#FF020202" HorizontalAlignment="Left" Height="199" Margin="21,10,0,0" Stroke="Black" RadiusY="16.098" RadiusX="16.098" VerticalAlignment="Top" Width="399"/>
        
		<Button Style="{DynamicResource GlassButton}" Margin="128,136.318,153.833,48.182" Content="OK" RenderTransformOrigin="0.5,0.5" Click="Button_Click_2"/>
        <Label Content="{Binding pText}" HorizontalAlignment="Left" Margin="37,29,0,0" VerticalAlignment="Top" Foreground="White" FontSize="21.333" Height="93.75" Width="360.703"/>

    </Grid>
</Window>
